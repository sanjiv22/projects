 <h1>Listing article</h1>
 <table>
 	<tr>
 		<th> Title </th>
 		<th>Text</th>
 	</tr>
 	<% @articles.each do [article] %>
 	<tr>
 		<td><%= article.title %> </td>
 		<td><%= article.text %></td>
 		<td><%= link to 'show', article_path(article) %></td>
 	</tr>
<% end %>
</table>
<%= link to 'New article', new_article_path %>